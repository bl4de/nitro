{
    "env": {
        "browser": false,
        "node": true,
        "es6": true
    },
    "rules": {
        "camelcase": 2,
        "quotes": [
            1,
            "single"
        ],
        "semi": 0,
        "no-var": 2,
        "no-lone-blocks": 2,
        "no-redeclare": 2,
        "no-empty": 2,
        "no-unreachable": 2,
        "valid-jsdoc": 0
    }
}